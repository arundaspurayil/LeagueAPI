doctype html
include ./championinfo.pug
html
  head
    title() GameAbilities.gg
    link(rel='stylesheet', href='/stylesheets/match.css')

  body
    .container.match-container
      if(match)
        .row
          .col
            h1() Red Team
            each user in match.participants
              if(user.teamId == 100)
                
                  +championinfo(user)
          .col
            h1() Blue Team
            each user in match.participants
                if(user.teamId == 200)
                  
                    +championinfo(user)
                
          br



